package array_dsa;

public class Program1 {
	public static void main(String[] args) {
		int []a= {10,30,8,6,7,8};
		int c;
		for(int i=0;i<a.length;i++) {
			//System.out.println(a[i]);
		}
		for(int i=0;i<a.length;i++) {
			if(i==3) {
				int temp=a[i];
				a[i]=40;
				c=a[i+1];
				a[i+1]=temp;
			}
		
			System.out.println(a[i]);
		}
		
	}
	}
