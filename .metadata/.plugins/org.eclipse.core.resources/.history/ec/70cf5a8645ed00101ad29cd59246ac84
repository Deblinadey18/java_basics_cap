package flowstatement_programs.looping_tmt.whileloop;

public class Program8 {

	public static void main(String[]args) {
		int num=28;
		int sum=0;
		int i=1;
		while(i<num) {
			if(num%i==0) {
				sum=sum+i;
				
			}
			i=i+1;
		}
		if(sum==num) {
			System.out.println("perfect");
		}
		else {
			System.out.println("not perfect");
		}
	}
}
