package user_input;
import java.util.*;
public class Program4 {
public static void main(String[] args) {
	Scanner scan=new Scanner(System.in);
	System.out.println("Enter the length of the array");
	int n= scan.nextInt();
	int[]a=new int[n];
	for(int i=0;i<n;i++) {
		System.out.println("Enter the elements4 of the array");
		a[i]=scan.nextInt();
	}
	for(int i=0;i<n;i++) {
		for(int j=i;j<n;j++) {
			for(int k=i;k<=j;k++) {
				System.out.print(a[k]);

			}
			System.out.println("");
		}
	}
}
}
