package examthirdweek;
import java.util.*;
public class ArrayProduct {
	public static void main(String[]args) {
		Scanner sc = new Scanner(System.in);
	    int n = sc.nextInt();
	    int n1=n;
		int[] arr=new int[n];
		int[] arr2=new int[n1];
		for(int i=0;i<n;i++) {
			arr[i]= sc.nextInt();	
		}
		for(int j=0;j<n;j++) {
			int product=1;
		
		for(int i=0;i<n;i++) {
			if(i!=j) {
			product=product*arr[i];
		}
			}
		arr2[j]=product;
		}
		for(int j=0;j<n1;j++) {
			System.out.print(arr2[j]);
		}
		
	}
	
}

