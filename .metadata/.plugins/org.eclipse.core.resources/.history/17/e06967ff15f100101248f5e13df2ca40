package oops.fundamental.non_static.methods;

public class Person {

	String name;
	int age;
	String gender;
	public void display() {
		System.out.println("this:"+this);
		System.out.println("Name:"+name);
		System.out.println("Age:"+age);
		System.out.println("Gendaer:"+gender);

	}
	public void initialize(String name,int age,String gender) {
		System.out.println("this:"+this);
		this.name=name;
		this.age=age;
		this.age=age;
	}
	
}
