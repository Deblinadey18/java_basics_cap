package examthirdweek;

public class RotateArray {
    public static void main(String[] args) {
        int[] a = {1,2,3,4,5,6,7};
        int k = 3;
        int n = a.length;
        int n1=n;
		int[] arr2=new int[n1];
		for (int j=0;j<n1;)
        for (int i = k; i < n1; i++) {
            arr2[j]=a[i];}

        for (int i=0;i<n1;i++)
            System.out.print(arr2[i]);
    }
}
