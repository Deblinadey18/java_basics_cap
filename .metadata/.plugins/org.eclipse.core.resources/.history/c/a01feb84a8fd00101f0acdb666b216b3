package array_dsa;

public class Program1 {
	public static void main(String[] args) {
		int []a= {10,30,8,6,7,8};
		int []b=new int[7];
		for(int i=0;i<a.length;i++) {
			//System.out.println(a[i]);
		}
		for(int i=0;i<7;i++) {
			if(i==3) {
				b[i]=40;
			}
			else if(i>3) {
				b[i]=a[i];
			}
			else {
			b[i]=a[i];}
			System.out.println(a[i]);
		}
		
	}
	}