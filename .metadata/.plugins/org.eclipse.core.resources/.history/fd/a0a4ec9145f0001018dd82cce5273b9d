package methods;

public class Program6 {

	public static void  main(String[]args) {
		perfectsquare(4);
	}
	public static void  perfectsquare(int n) {
		int count=0;//1
		for(int i=1;i<=n;i++) {
			if(i*i==n) {
				count++;
				break;}
				if(count==1) {
					System.out.println("Its Perfect Square");
				}
				else{
					System.out.println("Its  not Perfect Square");
				}
			}
		}
		
	}

