package association;

public class Car2 {

	//new Car()
	private String brand;//null
	private double price;//0.0
	
	
	private association.tyre tyre;//null
	
	Car2(){
		tyre=new tyre();
	}
	
	public String getBrand() {
		return brand;
	}
	
	public double getPrice() {
		return price;
	}
	
	public tyre gettyre(){
		return tyre;
	}

	public void setBrand(String brand) {
		this.brand = brand;
	}

	public void setPrice(double price) {
		this.price = price;
	}

	public void setEngine(Engine engine) {
		this.engine = engine;
	}
	
	
}