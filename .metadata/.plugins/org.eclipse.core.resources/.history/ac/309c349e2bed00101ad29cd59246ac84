package flowstatement_programs.Decisionstmt;

public class Simple_if_program {
	public static void main(String[]args) {
		if(true) {
			System.out.println("1");
			System.out.println("2");

			
		}
		System.out.println("Main End");
		if(false) {
			System.out.println("1");
			System.out.println("2");

			
		}
		System.out.println("Main End");


	}

}
