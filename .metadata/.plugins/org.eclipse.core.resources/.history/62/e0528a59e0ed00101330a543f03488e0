package flowstatement_programs.looping_tmt.forloop;

public class program7 {

		public static void main(String[]args) {
			for(int row=1;row<=3;row++) {
				for(int col=1;col<=3;col++) {
					if(col==row)
					    System.out.print("*");
					else
						System.out.print(' ');

				}
				System.out.println();
			}
		}
	}


}
