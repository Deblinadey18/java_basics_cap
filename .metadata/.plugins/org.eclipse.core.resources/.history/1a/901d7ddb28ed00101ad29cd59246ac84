package operator.compoundAssignmentOperator;

public class Program1 {
	public static void main(String[]args) {
		int a=10;
		a+=10;//a=a+10=20
		a+=10.5;//a=a+10.5=20+10.5=30(as it is int)
		System.out.println(a);
		int b=10;
		b%=10;//a=a+10=20
		b%=10.5;//a=a+10.5=20+10.5=30(as it is int)
		System.out.println(b);
	}

}
