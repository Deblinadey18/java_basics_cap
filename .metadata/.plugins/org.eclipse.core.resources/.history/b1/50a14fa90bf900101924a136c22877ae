package examthirdweek;

class RotateArray {
    public static void main(String[] args) {
        int[] a = {1,2,3,4,5,6,7};
        int k = 3;
        int n = a.length;

        for (int i = 0; i < k+1; i++) {
            int first = a[0];
            for (int j = 0; j < n - 1; j++)
                a[j] = a[j + 1];
            a[n - 1] = first;
        }

        for (int i = 0; i <n; i++) {
            System.out.print(a[i]);
    }
}}
    
