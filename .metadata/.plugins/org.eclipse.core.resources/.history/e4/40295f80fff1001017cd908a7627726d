package assignment15jan;

public class Occurance {

	public static void main(String[]args) {
		String a1="hello";
		int len=a1.length();
		char[]ch1 = a1.toCharArray();
		char [] ch2=new char[len];
		for(int i=0;i<len;i++) {
			int c=0;
			for(int j=0;j<=i;j++) {
				if(ch2[j]==ch1[i]) {
					if(c!=0) {
					c=c+1;
					}
				}
				else {
					  ch2[j]=ch1[i];
					  c=1;
                      break;
				}
				
			}
		
		System.out.println("The occurance of "+ch1[i]+" is "+c);}
		}
	}
