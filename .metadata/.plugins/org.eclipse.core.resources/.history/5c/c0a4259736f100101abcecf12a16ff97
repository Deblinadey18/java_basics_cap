package array;

public class Program8 {
 public static void main(String[]args) {
	 int[]a= {2,5,3,7,10,4};
	 int max=a[0];
	 int min=a[0];
	 for(int i=0;i<a.length;i++) {
		 if(a[i]>max) {
			 max=a[i];
		 }
		 else if(a[i]<min) {
			 min=a[i];

		 }
	 }
	 int diff;
	 diff=max-min;
	 System.out.println(diff);
	 
 }
}
