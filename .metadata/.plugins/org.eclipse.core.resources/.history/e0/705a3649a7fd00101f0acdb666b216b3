package array_dsa;

public class Program1 {
	public static void main(String[] args) {
		int []a= {10,30,8,6,7,8};
		int []b=new int[a.length+1];
		for(int i=0;i<a.length;i++) {
			//System.out.println(a[i]);
		}
		for(int i=0;i<b.length;i++) {
			if(i==3) {
				b[i]=40;
				break;
			}
			b[i]=a[i];
		
			System.out.println(a[i]);
		}
		
	}
	}
